{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": false,
      "strict": false,
      "module": "esnext",
      "target": "es5",
      "jsx": "react",
      "allowJs": false,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "downlevelIteration": true,
      "baseUrl": "./src",
      "lib": [
        "dom",
        "es5",
        "es2015.promise"
      ]
    }
}